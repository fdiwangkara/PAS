<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_color"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    tools:context=".HomeActivity"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/main_color"
        >


        <ImageView

            android:contentDescription="@string/content_desc"
            android:id="@+id/imageView2"
            android:layout_width="359dp"
            android:layout_height="202dp"
            android:src="@drawable/icon_home_fruit"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/sub_congrats"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="70dp"
            android:fontFamily="@font/bentonbold"
            android:lineHeight="45dp"
            android:text="@string/find_your_favorite_food"
            android:textAlignment="textStart"
            android:textColor="@color/white"
            android:textSize="35sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="TextSizeCheck" />
        <com.google.android.material.button.MaterialButton

            app:iconTint="@color/ui_color"
            app:icon="@drawable/baseline_notifications_24"
            app:iconGravity="textTop"
            android:text="@string/test"
            app:iconSize="25dp"
            android:id="@+id/google"
            android:layout_width="60dp"
            android:layout_height="67dp"
            android:layout_gravity="center"
            android:layout_marginTop="77dp"
            android:layout_marginEnd="50dp"
            android:backgroundTint="#252525"
            android:paddingLeft="37dp"
            android:paddingTop="40dp"
            android:paddingRight="37dp"
            android:paddingBottom="20dp"
            android:textSize="19sp"
            app:cornerRadius="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/sub_congrats" />


        <com.google.android.material.textfield.TextInputLayout
            android:layout_marginTop="30dp"
            android:id="@+id/user_Password"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="285dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/bentonregular"
            android:hint="@string/what_do_you_want_to_order"
            android:textColorHighlight="@color/white"
            android:textColorHint="#555050"
            app:startIconDrawable="@drawable/search"
            app:startIconTint="@color/white"
            app:hintTextColor="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sub_congrats">

            <com.google.android.material.textfield.TextInputEditText

                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="62.5dp"
                android:background="@drawable/edt_input"
                android:cursorVisible="false"
                android:fontFamily="@font/bentonregular"
                android:paddingStart="20dp"
                android:textColor="#92929D"
                android:textSize="15sp"
                tools:ignore="RtlSymmetry" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.button.MaterialButton

            android:id="@+id/setting"
            android:layout_width="65dp"
            android:layout_height="72dp"
            android:layout_gravity="center"
            android:layout_marginTop="31dp"
            android:layout_marginEnd="25dp"
            android:backgroundTint="#252525"
            android:contentDescription="@string/content_desc"
            android:fontFamily="@font/bentosanmedium"
            android:gravity="start"
            android:paddingLeft="37dp"
            android:paddingTop="20dp"
            android:paddingRight="37dp"
            android:paddingBottom="20dp"
            android:textSize="19sp"
            app:cornerRadius="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/sub_congrats"
            tools:ignore="DuplicateSpeakableTextCheck" />



        <androidx.cardview.widget.CardView
            android:id="@+id/card_view3"
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:background="#2B2A2A"
            android:backgroundTint="#2B2A2A"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/user_Password"
            card_view:cardCornerRadius="22dp">

            <androidx.constraintlayout.widget.ConstraintLayout

                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:background="#2B2A2A"

                android:backgroundTint="#2B2A2A">


                <androidx.cardview.widget.CardView
                    android:id="@+id/card_view4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="18dp"
                    android:background="#2B2A2A"
                    android:backgroundTint="#2B2A2A"
                    app:cardCornerRadius="15dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#2B2A2A">

                        <ImageView


                            android:id="@+id/img_item_photo"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:contentDescription="@string/content_desc"
                            android:scaleType="centerCrop"
                            card_view:layout_constraintBottom_toBottomOf="parent"
                            card_view:layout_constraintStart_toStartOf="parent"
                            card_view:layout_constraintTop_toTopOf="parent"
                            tools:ignore="ImageContrastCheck"
                            tools:src="@tools:sample/avatars" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/Green"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="25dp"
                    android:fontFamily="@font/bentosanmedium"
                    android:text="@string/spacy_fresh_crab"
                    android:textColor="#FFFFFF"
                    android:textSize="17.5sp"
                    app:layout_constraintStart_toEndOf="@+id/card_view4"
                    app:layout_constraintTop_toTopOf="@+id/card_view4" />

                <ImageView

                    android:contentDescription="@string/content_desc"
                    android:id="@+id/iconmaps"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="25dp"
                    app:layout_constraintStart_toEndOf="@id/card_view4"
                    app:layout_constraintTop_toBottomOf="@id/Green"
                    android:src="@drawable/Iconmaps"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:id="@+id/Noodle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="9dp"
                    android:layout_marginTop="6dp"

                    android:fontFamily="@font/bentonregular"
                    android:text="@string/_25_minutes_on_the_way"
                    android:textColor="#3B3B3B"
                    android:textSize="13sp"
                    app:layout_constraintStart_toEndOf="@+id/iconmaps"
                    app:layout_constraintTop_toBottomOf="@+id/Green"
                    tools:ignore="TextSizeCheck,TextContrastCheck" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bentosanmedium"
                    android:text="@string/_29"
                    android:textColor="@color/ui_color"
                    android:textSize="19sp"
                    app:layout_constraintStart_toStartOf="@+id/Green"
                    app:layout_constraintTop_toBottomOf="@id/Noodle"
                    android:layout_marginTop="10dp"/>


            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/martabakManis"
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:background="#2B2A2A"
            android:backgroundTint="#2B2A2A"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/card_view3"
            card_view:cardCornerRadius="22dp">

            <androidx.constraintlayout.widget.ConstraintLayout

                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:background="#2B2A2A"

                android:backgroundTint="#2B2A2A">


                <androidx.cardview.widget.CardView
                    android:id="@+id/foodCard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="18dp"
                    android:background="#2B2A2A"
                    android:backgroundTint="#2B2A2A"
                    app:cardCornerRadius="15dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#2B2A2A">

                        <ImageView

                            android:contentDescription="@string/content_desc"
                            android:id="@+id/foodPhoto"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:scaleType="centerCrop"
                            card_view:layout_constraintBottom_toBottomOf="parent"
                            card_view:layout_constraintStart_toStartOf="parent"
                            card_view:layout_constraintTop_toTopOf="parent"
                            tools:src="@tools:sample/avatars" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/foodName2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="25dp"
                    android:fontFamily="@font/bentosanmedium"
                    android:text="@string/martabak_manis"
                    android:textColor="#FFFFFF"
                    android:textSize="17.5sp"
                    app:layout_constraintStart_toEndOf="@+id/foodCard"
                    app:layout_constraintTop_toTopOf="@+id/foodCard" />

                <ImageView

                    android:contentDescription="@string/content_desc"
                    android:id="@+id/iconmaps2"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="25dp"
                    app:layout_constraintStart_toEndOf="@id/foodCard"
                    app:layout_constraintTop_toBottomOf="@id/foodName2"
                    android:src="@drawable/Iconmaps"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:id="@+id/otw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="9dp"
                    android:layout_marginTop="6dp"

                    android:fontFamily="@font/bentonregular"
                    android:text="@string/_12_minutes_on_the_way"
                    android:textColor="#3B3B3B"
                    android:textSize="13sp"
                    app:layout_constraintStart_toEndOf="@+id/iconmaps2"
                    app:layout_constraintTop_toBottomOf="@+id/foodName2"
                    tools:ignore="TextSizeCheck,TextContrastCheck" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bentosanmedium"
                    android:text="@string/_12"
                    android:textColor="@color/ui_color"
                    android:textSize="19sp"
                    app:layout_constraintStart_toStartOf="@+id/foodName2"
                    app:layout_constraintTop_toBottomOf="@id/otw"
                    android:layout_marginTop="10dp"/>


            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/nasiGoreng"
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:background="#2B2A2A"
            android:backgroundTint="#2B2A2A"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/martabakManis"
            card_view:cardCornerRadius="22dp">

            <androidx.constraintlayout.widget.ConstraintLayout

                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:background="#2B2A2A"

                android:backgroundTint="#2B2A2A">


                <androidx.cardview.widget.CardView
                    android:id="@+id/foodCard3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="18dp"
                    android:background="#2B2A2A"
                    android:backgroundTint="#2B2A2A"
                    app:cardCornerRadius="15dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#2B2A2A">

                        <ImageView

                            android:id="@+id/foodPhoto3"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:contentDescription="@string/content_desc"
                            android:scaleType="centerCrop"
                            card_view:layout_constraintBottom_toBottomOf="parent"
                            card_view:layout_constraintStart_toStartOf="parent"
                            card_view:layout_constraintTop_toTopOf="parent"
                            tools:ignore="ImageContrastCheck"
                            tools:src="@tools:sample/avatars" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/foodName3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="25dp"
                    android:fontFamily="@font/bentosanmedium"
                    android:text="@string/nasi_goreng"
                    android:textColor="#FFFFFF"
                    android:textSize="17.5sp"
                    app:layout_constraintStart_toEndOf="@+id/foodCard3"
                    app:layout_constraintTop_toTopOf="@+id/foodCard3" />


                <TextView
                    android:id="@+id/done"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"

                    android:fontFamily="@font/bentonregular"
                    android:text="@string/your_order_finished"
                    android:textColor="#3B3B3B"
                    android:textSize="13sp"
                    app:layout_constraintStart_toStartOf="@+id/foodName3"
                    app:layout_constraintTop_toBottomOf="@+id/foodName3"
                    tools:ignore="TextSizeCheck,TextContrastCheck" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bentosanmedium"
                    android:text="@string/_45"
                    android:textColor="#BEBEBE"
                    android:textSize="19sp"
                    app:layout_constraintStart_toStartOf="@+id/foodName3"
                    app:layout_constraintTop_toBottomOf="@id/done"
                    android:layout_marginTop="10dp"/>


            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/baksoMalang"
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:background="#2B2A2A"
            android:backgroundTint="#2B2A2A"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/nasiGoreng"
            card_view:cardCornerRadius="22dp">

            <androidx.constraintlayout.widget.ConstraintLayout

                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:background="#2B2A2A"

                android:backgroundTint="#2B2A2A">


                <androidx.cardview.widget.CardView
                    android:id="@+id/foodCard4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="18dp"
                    android:background="#2B2A2A"
                    android:backgroundTint="#2B2A2A"
                    app:cardCornerRadius="15dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#2B2A2A">

                        <ImageView

                            android:contentDescription="@string/content_desc"
                            android:id="@+id/foodPhoto4"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:scaleType="centerCrop"
                            card_view:layout_constraintBottom_toBottomOf="parent"
                            card_view:layout_constraintStart_toStartOf="parent"
                            card_view:layout_constraintTop_toTopOf="parent"
                            tools:src="@tools:sample/avatars" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/foodName4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="25dp"
                    android:fontFamily="@font/bentosanmedium"
                    android:text="@string/bakso_malang"
                    android:textColor="#FFFFFF"
                    android:textSize="17.5sp"
                    app:layout_constraintStart_toEndOf="@+id/foodCard4"
                    app:layout_constraintTop_toTopOf="@+id/foodCard4" />


                <TextView
                    android:id="@+id/done2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"

                    android:fontFamily="@font/bentonregular"
                    android:text="@string/your_order_finished"
                    android:textColor="#3B3B3B"
                    android:textSize="13sp"
                    app:layout_constraintStart_toStartOf="@+id/foodName4"
                    app:layout_constraintTop_toBottomOf="@+id/foodName4"
                    tools:ignore="TextSizeCheck,TextContrastCheck" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bentosanmedium"
                    android:text="@string/_32"
                    android:textColor="#BEBEBE"
                    android:textSize="19sp"
                    app:layout_constraintStart_toStartOf="@+id/foodName4"
                    app:layout_constraintTop_toBottomOf="@id/done2"
                    android:layout_marginTop="10dp"/>


            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/setLocButton"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:layout_marginStart="30dp"
            android:layout_marginTop="390dp"
            android:layout_marginEnd="30dp"
            android:backgroundTint="@color/ui_color"
            android:fontFamily="@font/bentonbold"
            android:paddingLeft="55dp"
            android:paddingRight="55dp"

            android:text="@string/set_location"
            android:textSize="14sp"
            app:cornerRadius="14dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/card_view3"
            tools:ignore="TextSizeCheck,VisualLintButtonSize" />
    </androidx.constraintlayout.widget.ConstraintLayout>



</ScrollView>